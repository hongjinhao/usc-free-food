{
    "buildCommand": "npm run build",
    "framework": "vite",
    "installCommand": "npm install",
    "outputDirectory": "dist",
    "functions": {
        "api/cron-scan-events.js": {
            "maxDuration": 300
        }
    },
    "crons": [
        {
            "path": "/api/cron-scan-events",
            "schedule": "0 10 * * *"
        }
    ]
}